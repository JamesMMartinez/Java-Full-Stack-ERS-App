<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 
 <web-app>
 
 <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
 </servlet>
 <servlet>
        <servlet-name>LoggedInServlet</servlet-name>
        <servlet-class>com.revature.servlets.LoggedInServlet</servlet-class>
 </servlet>
 <servlet>
        <servlet-name>ReimbursementServlet</servlet-name>
        <servlet-class>com.revature.servlets.ReimbursementServlet</servlet-class>
 </servlet>
 <servlet>
        <servlet-name>ManagerReimbursementServlet</servlet-name>
        <servlet-class>com.revature.servlets.ManagerReimbursementServlet</servlet-class>
 </servlet>
 <servlet>
        <servlet-name>EmployeeReimbursementServlet</servlet-name>
        <servlet-class>com.revature.servlets.EmployeeReimbursementServlet</servlet-class>
 </servlet>
 <servlet>
        <servlet-name>AllRequestsServlet</servlet-name>
        <servlet-class>com.revature.servlets.AllRequestsServlet</servlet-class>
 </servlet>
  <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.revature.servlets.UserServlet</servlet-class>
 </servlet>
 <servlet>
        <servlet-name>ManagerUserServlet</servlet-name>
        <servlet-class>com.revature.servlets.ManagerUserServlet</servlet-class>
 </servlet>
 <servlet>
        <servlet-name>EmployeeUserServlet</servlet-name>
        <servlet-class>com.revature.servlets.EmployeeUserServlet</servlet-class>
 </servlet>
 <servlet>
        <servlet-name>EmployeeUpdateServlet</servlet-name>
        <servlet-class>com.revature.servlets.EmployeeUpdateServlet</servlet-class>
 </servlet>
 
 
 
 <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
        <servlet-name>LoggedInServlet</servlet-name>
        <url-pattern>/loggedin</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
        <servlet-name>ReimbursementServlet</servlet-name>
        <url-pattern>/reimbursement</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
        <servlet-name>ManagerReimbursementServlet</servlet-name>
        <url-pattern>/manager-reimbursement</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
        <servlet-name>EmployeeReimbursementServlet</servlet-name>
        <url-pattern>/employee-reimbursement</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
        <servlet-name>AllRequestsServlet</servlet-name>
        <url-pattern>/all-requests</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/user</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
        <servlet-name>ManagerUserServlet</servlet-name>
        <url-pattern>/manager-user</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
        <servlet-name>EmployeeUserServlet</servlet-name>
        <url-pattern>/employee-user</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
        <servlet-name>EmployeeUpdateServlet</servlet-name>
        <url-pattern>/employee-update</url-pattern>
 </servlet-mapping>
    
    
    
 
 
 </web-app>